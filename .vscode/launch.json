    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch",
            "cargo": {
                "args": ["build"],
                "filter": {
                    "name": "day6",
                    "kind": "bin"
                }
            },
            "program": "${workspaceFolder}/target/debug/day6",
            "args": ["--chunksize", "5", "--file", "data/day6", "--threads", "2"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo Test Debug",
            "cargo": {
                "args": ["build", "test"],
                "filter": {
                    "name": "day6",
                    "kind": "bin"
                }
            },
            "cwd": "${workspaceFolder}"
        }
    ]
}